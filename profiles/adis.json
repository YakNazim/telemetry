[
    {
        "type": "metric", "name": "FC Packet Statistics",
        "x": 4, "y": 1, "sx":2, "sy": 1,
        "lines":  [
            {"name": "Time since last FC packet",   "data": "d.servertime - d.RECV_fc.TimeLastPacketReceived",  "drift": "minor", "spark": "False"},
            {"name": "Dropped FC packets",          "data": "d.RECV_fc.PacketsLostRecently",                    "drift": "minor", "spark": "False"},
            {"name": "Packet Rate",                 "data": "d.RECV_fc.PacketsReceivedRecently / 0.1",          "drift": "minor", "spark": "False"}
        ]
    },
    {
        "type": "metric",
        "name": "ADIS Vertical",
        "x": 1, "y": 1, "sx":3, "sy": 2,
        "lines": [
            {"name": "Vertical Acceleration [m/s&sup2;]", "data": "d.ADIS.Acc_X_mean",                          "drift": "key", "spark": "True"},
            {"name": "Roll Rate [&deg;/s]",               "data": "d.ADIS.Gyro_X_mean",                         "drift": "key", "spark": "True"},
            {"name": "Roll Magnitometer [uT]",            "data": "d.ADIS.Magn_X_mean",                         "drift": "key", "spark": "True"}
        ]
    },
    {
        "type": "metric",
        "name": "Other data",
        "x": 4, "y": 2, "sx":2, "sy": 2,
        "lines": [
            {"name": "Temperature [&deg;C]",   "data": "d.ADIS.Temp_mean",                                  "drift": "major", "spark": "False"},
            {"name": "divider" },
            {"name": "Y-Mag",                  "data": "d.ADIS.Magn_Y_mean",                                "drift": "minor", "spark": "False"},
            {"name": "Z-Mag",                  "data": "d.ADIS.Magn_Z_mean",                                "drift": "minor", "spark": "False"},
            {"name": "divider" },
            {"name": "Voltage [V]",            "data": "d.ADIS.VCC_mean",                                   "drift": "minor", "spark": "False"}
        ]
    },
    {
        "type": "metric",
        "name": "Accel",
        "x": 1, "y": 3, "sx":1, "sy": 1,
        "lines": [
            {"name": "Y-Accel",                "data": "d.ADIS.Acc_Y_mean",                                 "drift": "minor", "spark": "False"},
            {"name": "Z-Accel",                "data": "d.ADIS.Acc_Z_mean",                                 "drift": "minor", "spark": "False"}
        ]
    },
    {
        "type": "metric",
        "name": "Gyro",
        "x": 2, "y": 3, "sx":1, "sy": 1,
        "lines": [
            {"name": "Y-Gyro",                "data": "d.ADIS.Gyro_Y_mean",                                 "drift": "minor", "spark": "False"},
            {"name": "Z-Gyro",                "data": "d.ADIS.Gyro_Z_mean",                                 "drift": "minor", "spark": "False"}
        ]
    },
    {
        "type": "metric",
        "name": "Magnitudes",
        "x": 3, "y": 3, "sx":1, "sy": 1,
        "lines": [
            {"name": "Accelometer",           "data": "Math.sqrt((d.ADIS.Acc_X_mean*d.ADIS.Acc_X_mean)+(d.ADIS.Acc_Y_mean*d.ADIS.Acc_Y_mean)+(d.ADIS.Acc_Z_mean*d.ADIS.Acc_Z_mean))", "drift": "minor", "spark": "False"},
            {"name": "Magnetometer",          "data": "Math.sqrt((d.ADIS.Magn_X_mean*d.ADIS.Magn_X_mean)+(d.ADIS.Magn_Y_mean*d.ADIS.Magn_Y_mean)+(d.ADIS.Magn_Z_mean*d.ADIS.Magn_Z_mean))", "drift": "minor", "spark": "False"}
        ]
    }
]
