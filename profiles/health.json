[
    {
        "type": "metric",
        "name": "Power",
        "x": 1, "y": 1, "sx":3, "sy": 4,
        "lines": [
            {"name": "Battery Pack Voltage",        "data": "d.RNHH.Voltage",                                   "drift": "key",   "spark": "True"},
            {"name": "Battery Pack Temp",           "data": "d.RNHH.Temperature",                               "drift": "key",   "spark": "True"},
            {"name": "Battery Pack Current",        "data": "d.RNHH.Current",                                   "drift": "key",   "spark": "True"},
            {"name": "divider" },
            {"name": "Cell 1 Voltage",              "data": "d.RNHH.CellVoltage1",                                "drift": "minor", "spark": "False"},
            {"name": "Cell 2 Voltage",              "data": "d.RNHH.CellVoltage2",                                "drift": "minor", "spark": "False"},
            {"name": "Cell 3 Voltage",              "data": "d.RNHH.CellVoltage3",                                "drift": "minor", "spark": "False"},
            {"name": "Cell 4 Voltage",              "data": "d.RNHH.CellVoltage4",                                "drift": "minor", "spark": "False"}
        ]
    },
    {
        "type": "metric", "name": "FC Packet Statistics",
        "x": 3, "y": 1, "sx":2, "sy": 1,
        "lines":  [
            {"name": "Time since last FC packet",   "data": "d.servertime - d.RECV_fc.TimeLastPacketReceived",  "drift": "minor", "spark": "False"},
            {"name": "Dropped FC packets",          "data": "d.RECV_fc.PacketsLostRecently",                    "drift": "minor", "spark": "False"},
            {"name": "Packet Rate",                 "data": "d.RECV_fc.PacketsReceivedRecently / 0.1",          "drift": "minor", "spark": "False"}
        ]
    },
    {
        "type": "metric", "name": "Port Current Measurements",
        "x": 1, "y": 4, "sx":5, "sy":5,
        "lines": [
            {"name": "Port 1", "data": "1000 * d.RNHP.Port1 * 3.3/Math.pow(2, 12) * (63000.0 / 69800.0)", "drift": "key", "spark": "True"},
            {"name": "Port 2", "data": "1000 * d.RNHP.Port2 * 3.3/Math.pow(2, 12) * (63000.0 / 69800.0)", "drift": "key", "spark": "True"},
            {"name": "Port 3", "data": "1000 * d.RNHP.Port3 * 3.3/Math.pow(2, 12) * (63000.0 / 69800.0)", "drift": "key", "spark": "True"},
            {"name": "Port 4", "data": "1000 * d.RNHP.Port4 * 3.3/Math.pow(2, 12) * (63000.0 / 69800.0)", "drift": "key", "spark": "True"},
            {"name": "Port 5", "data": "1000 * d.RNHP.Port5 * 3.3/Math.pow(2, 12) * (63000.0 / 69800.0)", "drift": "key", "spark": "True"},
            {"name": "Port 6", "data": "1000 * d.RNHP.Port6 * 3.3/Math.pow(2, 12) * (63000.0 / 69800.0)", "drift": "key", "spark": "True"},
            {"name": "Port 7", "data": "1000 * d.RNHP.Port7 * 3.3/Math.pow(2, 12) * (63000.0 / 69800.0)", "drift": "key", "spark": "True"},
            {"name": "Port 8", "data": "1000 * d.RNHP.Port8 * 3.3/Math.pow(2, 12) * (63000.0 / 69800.0)", "drift": "key", "spark": "True"}
        ]
    }
]
